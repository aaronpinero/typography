<form class="form-demo" id="lists-demo">
	<div class="form-control form-control-type">
		<label>List Type</label>
		<label class="option" for="list-type-ordered"><input type="radio" name="list-type" id="list-type-ordered" value="ordered"> Ordered List</label>
		<label class="option" for="list-type-unordered"><input type="radio" name="list-type" id="list-type-unordered" value="unordered" checked="checked"> Unordered List</label>
	</div>
	<div class="form-control form-control-classes">
		<label>List Class</label>
		<label class="option" for="list-class-icons"><input type="checkbox" name="list-class" id="list-class-icons" value="list-icons"> .list-icons(-angle)</label>
		<label class="option" for="list-class-nobullet"><input type="checkbox" name="list-class" id="list-class-nobullet" value="list-nobullet"> .list-nobullet</label>
		<label class="option disabled" for="list-class-indented"><input type="checkbox" name="list-class" id="list-class-indented" value="list-indented" disabled="disabled"> .list-indented</label>
		<label class="option" for="list-class-cols2"><input type="checkbox" name="list-class" id="list-class-cols2" value="list-cols-2"> .list-cols-2</label>
		<label class="option" for="list-class-cols3"><input type="checkbox" name="list-class" id="list-class-cols3" value="list-cols-3"> .list-cols-3</label>
		<label class="option" for="list-class-grid2"><input type="checkbox" name="list-class" id="list-class-grid2" value="list-grid-2"> .list-grid-2</label>
		<label class="option" for="list-class-grid3"><input type="checkbox" name="list-class" id="list-class-grid3" value="list-grid-3"> .list-grid-3</label>
		<label class="option" for="list-class-grid4"><input type="checkbox" name="list-class" id="list-class-grid4" value="list-grid-4"> .list-grid-4</label>
		<label class="option" for="list-class-marginsoff"><input type="checkbox" name="list-class" id="list-class-marginsoff" value="tymargins-off"> .tymargins-off</label>
		<label class="option" for="list-class-flex"><input type="checkbox" name="list-class" id="list-class-flex" value="list-flex"> .list-flex</label>
		<label class="option disabled" for="list-class-flexcenter"><input type="checkbox" name="list-class" id="list-class-flexcenter" value="list-flex-center" disabled="disabled"> .list-flex-center</label>
		<label class="option disabled" for="list-class-flexright"><input type="checkbox" name="list-class" id="list-class-flexright" value="list-flex-right" disabled="disabled"> .list-flex-right</label>
	</div>
</form>
